{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/no-empty-object-type": ["error", {
      "allowEmptyInterfaces": true
    }],
    "react/no-children-prop": "warn",
    "react/jsx-key": "error"
  }
} 